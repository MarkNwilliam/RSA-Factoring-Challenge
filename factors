#!/bin/bash

while read n; do
    if [[ $(($n % 2)) -eq 0 ]]; then
        echo "$n=$(($n/2))*2"
    else
        for i in $(seq 3 $(($n/2))); do
            if [[ $(($n % $i)) -eq 0 ]]; then
                echo "$n=$i*$(($n/$i))"
                break
            fi
        done
    fi
done < $1

